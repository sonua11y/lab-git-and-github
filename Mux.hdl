/** 
 * Multiplexor:
 * out = a if sel == 0
         b otherwise
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel,b=sel,out=nsel); //Not
    Nand(a=a,b=nsel,out=c);
    Nand(a=b,b=sel,out=e);
    Nand(a=c,b=e,out=out);
}